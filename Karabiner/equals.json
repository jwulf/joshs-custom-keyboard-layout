{
    "title": "Josh: Dvorak-style equal_sign",
    "rules": [
        {
            "description": "Josh: Map ] + modifier -> equal_sign, like Dvorak",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "close_bracket",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ],
                            "optional": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "equal_sign"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "close_bracket",
                        "modifiers": {
                            "mandatory": [
                                "keypad_num_lock"
                            ],
                            "optional": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "equal_sign"
                        }
                    ]
                }
            ]
        }
    ]
}